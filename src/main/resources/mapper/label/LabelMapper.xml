<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.crown.mapper.label.LabelMapper">

    <select id="queryLabelIdsByCustomerId" resultType="Integer" parameterType="Integer">
        select label_id from label_customer where customer_id = #{id} and brand_id = 1;
    </select>
    <delete id="deleteLabelCustomerByLabelId" parameterType="Integer">
        DELETE FROM label_customer  WHERE label_id = #{id}
    </delete>
    <delete id="deleteByBrandId" parameterType="Integer">
        DELETE FROM label  WHERE brand_id = #{id}
    </delete>
</mapper>
